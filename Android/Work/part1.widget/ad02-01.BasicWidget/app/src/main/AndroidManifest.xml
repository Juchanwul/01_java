<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.administrator.basicwidget"
          xmlns:android="http://schemas.android.com/apk/res/android">
    
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
    
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RadioActivity">
        </activity>
        <activity android:name=".WidthButtonAtivity">
        </activity>
        <activity android:name=".HeightButtonAtivity">
        </activity>
        <activity android:name=".ColorButtonAtivity">
        </activity>
        <activity android:name=".VisibleAtivity">
        </activity>
        <activity android:name=".MarginAtivity">
        </activity>
        <activity android:name=".PaddingAtivity">
        </activity>
    </application>

</manifest>